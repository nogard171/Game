<sprites>
	<sprite name="grass">
		<shape>
			<vector x="0" y="0"/>
			<vector x="32" y="0"/>
			<vector x="32" y="32"/>
			<vector x="0" y="32"/>
		</shape>
		<texture>
			<vector x="32" y="0"/>
			<vector x="64" y="0"/>
			<vector x="64" y="32"/>
			<vector x="32" y="32"/>		
		</texture>
	</sprite>
	<sprite name="dirt">
		<shape inherit="grass"/>
		<texture>
			<vector x="0" y="32"/>
			<vector x="32" y="32"/>
			<vector x="32" y="64"/>
			<vector x="0" y="64"/>		
		</texture>
	</sprite>
	<sprite name="test">
		<shape inherit="grass"/>
		<texture>
			<vector x="32" y="32"/>
			<vector x="64" y="32"/>
			<vector x="64" y="64"/>
			<vector x="32" y="64"/>		
		</texture>
	</sprite>
</sprites>